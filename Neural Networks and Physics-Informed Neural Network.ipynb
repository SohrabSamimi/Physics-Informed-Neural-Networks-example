    "import matplotlib.pyplot as plt\n",
    "\n",
    "grid = torch.linspace(0,2*np.pi,200).view(200,1)\n",
    "cos_grid = torch.cos(grid)\n",
    "y_prediction = NN(grid).detach()\n",
    "\n",
    "plt.plot(grid,cos_grid,label='Cosine')\n",
